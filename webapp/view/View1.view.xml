<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.Innovation.ui5Project.controller.View1" displayBlock="true">
	<Shell id="shell">
		<SplitApp id="app" initialDetail="detail"
		initialMaster="master">
		<detailPages>
			<Page
				id="detail"
				title="Detail 1"
				backgroundDesign= "Solid"
				class="sapUiStdPage">
				<Label text="Detail page 1" />
				<Button text="Go to Detail page2" press=".onPressNavToDetail" />
			</Page>
		 
		</detailPages>
		<masterPages>
			<Page
				id="master"
				title="Master 1"
				backgroundDesign= "List"
				icon="sap-icon://action"
				class="sapUiStdPage">
				<List>
					<StandardListItem title="To Master2" type="Navigation" press=".onPressGoToMaster" />
				</List>
			</Page>
		</masterPages>
		</SplitApp>
	</Shell>
</mvc:View>